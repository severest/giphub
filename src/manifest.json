{
  "manifest_version": 2,
  "name": "Giphub",
  "author": "Everest, Sean <me@seaneverest.com>",
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "96": "96.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "64": "64.png"
    },
    "default_title": "Giphub"
  },
  "permissions": [
    "tabs",
    "https://github.com/*",
    "https://api.giphy.com/*"
  ]
}
